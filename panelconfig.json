{
  // "cli": "'grid; rows: 2; cols: 2; space: 2mm' --tabs 'fixed; width: 3mm; vcount: 2' --cuts 'mousebites; drill: 0.5mm; spacing: 1mm; offset: 0.2mm; prolong: 0.5mm' --framing 'frame; width: 5mm; space: 3mm; cuts: both' --post 'millradius: 1mm' --fiducials '3fid; hoffset: 5mm; voffset: 2.5mm; coppersize: 2mm; opening: 1mm;'",

  "layout": {
    "type": "grid",
    "rows": 2,
    "cols": 2,
    "space": "2mm"
  },

  "tabs": {
    "type": "fixed",
    "width": "3mm",
    "vcount": 1
  },

  "cuts": {
    "type": "mousebites",
    "drill": "0.5mm",
    "spacing": "1mm",
    "offset": "0.2mm",
    "prolong": "0.5mm"
  },

  "framing": {
    "type": "frame",
    "width": "5mm",
    "space": "3mm",
    "cuts": "both",
    "fillet": "2mm"
  },

  "post": {
    "millradius": "1mm"
  },

  "fiducials": {
    "type": "3fid",
    "hoffset": "5mm",
    "voffset": "2.5mm",
    "coppersize": "1mm",
    "opening": "2mm"
  }
}
